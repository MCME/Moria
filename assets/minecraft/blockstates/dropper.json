{
  "multipart": [
    {
      "when": {
        "facing": "down",
        "triggered": "false"
      },
      "apply": {
        "model": "block/dropper_vertical",
        "x": 180
      }
    },
    {
      "when": {
        "facing": "up",
        "triggered": "false"
      },
      "apply": {
        "model": "block/dropper_vertical"
      }
    },
    {
      "when": {
        "facing": "north",
        "triggered": "false"
      },
      "apply": {
        "model": "block/dropper"
      }
    },
    {
      "when": {
        "facing": "south",
        "triggered": "false"
      },
      "apply": {
        "model": "block/dropper",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "west",
        "triggered": "false"
      },
      "apply": {
        "model": "block/dropper",
        "y": 270
      }
    },
    {
      "when": {
        "facing": "east",
        "triggered": "false"
      },
      "apply": {
        "model": "block/dropper",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "down",
        "triggered": "true"
      },
      "apply": {
        "model": "block/rope_block"
      }
    },
    {
      "when": {
        "facing": "up",
        "triggered": "true"
      },
      "apply": {
        "model": "block/chain_block"
      }
    },
    {
      "when": {
        "facing": "north",
        "triggered": "true"
      },
      "apply": {
        "model": "block/rope_block",
        "x": 90
      }
    },
    {
      "when": {
        "facing": "south",
        "triggered": "true"
      },
      "apply": {
        "model": "block/chain_block",
        "x": 90
      }
    },
    {
      "when": {
        "facing": "west",
        "triggered": "true"
      },
      "apply": {
        "model": "block/rope_block",
        "y": 90,
        "x": 90
      }
    },
    {
      "when": {
        "facing": "east",
        "triggered": "true"
      },
      "apply": {
        "model": "block/chain_block",
        "y": 90,
        "x": 90
      }
    }
  ]
}
